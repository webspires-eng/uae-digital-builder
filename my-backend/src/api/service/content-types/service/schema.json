{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": false
    },
    "title": {
      "type": "string"
    },
    "short": {
      "type": "blocks"
    },
    "description": {
      "type": "blocks"
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "serviceicon": {
      "type": "customField",
      "customField": "plugin::strapi-plugin-iconhub.iconhub",
      "options": {
        "storeIconData": true,
        "storeIconName": true
      }
    }
  }
}
